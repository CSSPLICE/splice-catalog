{
  "name": "splice-catalog",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc --project ./tsconfig.json",
    "dev-works": "node --trace-warnings --experimental-specifier-resolution=node --loader ts-node/esm/transpile-only src/index.ts",
    "dev-old": "yarn nodemon --watch src -L --exec 'ts-node-esm' -r tsconfig-paths/register src/index.ts",
    "dev": "nodemon --watch src --exec bash -c 'yarn build && node dist/index.js'",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**.ts --fix",
    "pretty": "prettier . --write",
    "gulp": "gulp",
    "typeorm": "typeorm-ts-node-esm -d src/db/data-source.ts",
    "typeorm-cli": "ts-node-esm",
    "migrate:show": "yarn typeorm migration:show",
    "migrate:create": "node scripts/migration-create.js",
    "migrate": "yarn typeorm migration:run",
    "migrate:revert": "yarn typeorm migration:revert",
    "import:ontology": "ts-node-esm src/utils/import-ontology-data.ts"
  },
  "dependencies": {
    "@adminjs/express": "^6.1.1",
    "@adminjs/typeorm": "^5.0.1",
    "@types/cors": "^2.8.16",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "adminjs": "^7.8.17",
    "axios": "^1.7.9",
    "bunyan": "^1.8.15",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-formidable": "^1.2.0",
    "express-openid-connect": "^2.17.1",
    "express-session": "^1.18.2",
    "gulp": "^5.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "lemmatizer": "^0.0.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.12.0",
    "natural": "^8.0.1",
    "pg": "^8.13.1",
    "puppeteer": "^24.8.1",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "string-similarity": "^4.0.4",
    "typeorm": "^0.3.20",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@types/node": "^22.10.10",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "ejs-lint": "^2.0.1",
    "eslint": "^9.18.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "resolutions": {
    "@types/express": "^4.17.21"
  }
}
